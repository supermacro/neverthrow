{
  "name": "neverthrow",
  "version": "6.1.0",
  "description": "Stop throwing errors, and instead return Results!",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest && npm run test-types",
    "test-types": "tsc --noEmit -p ./tests/tsconfig.tests.json",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write 'src/**/*.ts?(x)' && npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ./dist ./tmp",
    "build": "npm run clean && rollup --config && mv tmp/*.js dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermacro/neverthrow.git"
  },
  "author": "Giorgio Delgado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supermacro/neverthrow/issues"
  },
  "homepage": "https://github.com/supermacro/neverthrow#readme",
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.8",
    "@babel/preset-typescript": "7.16.7",
    "@types/jest": "27.4.1",
    "@types/node": "14.17.4",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "babel-jest": "27.5.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "27.5.1",
    "prettier": "2.2.1",
    "rollup": "2.75.6",
    "rollup-plugin-dts": "3.0.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "testdouble": "3.16.3",
    "ts-jest": "27.1.5",
    "ts-toolbelt": "9.6.0",
    "tslib": "^2.4.0",
    "typescript": "4.7.2"
  },
  "keywords": [
    "typescript",
    "functional",
    "fp",
    "error"
  ]
}
