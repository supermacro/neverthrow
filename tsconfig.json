{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": [
      "ES2016",
      "ES2017.Object"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": [
    "src/**/*.ts",
  ],
  "exclude": [],
  "references": [
    {
      "path": "./tsconfig.tests.json"
    }
  ]
}